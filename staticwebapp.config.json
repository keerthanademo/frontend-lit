{
  "routes": [
    {
      "route": "//confirm",
      "methods": ["GET"],
      "rewrite": "/api/confirm"
    },
    {
      "route": "/api/subscribe",
      "methods": ["POST"],
      "rewrite": "/api/subscribe"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/assets/*", "/css/*", "/api/*"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "globalHeaders": {
    "Access-Control-Allow-Origin": "https://www.luxuryintaste.com",
    "Access-Control-Allow-Methods": "GET, POST, OPTIONS, PUT, DELETE",
    "Access-Control-Allow-Headers": "Content-Type"
  }
}
